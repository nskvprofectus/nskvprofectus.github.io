
<ul class="social-icons flex">

    {% for icon, link in site.social_links %}

    <li>
        <a href="{{ link | safe }}" target="_blank" rel="noreferrer noopener">
            <img
                src="/media/social_media_icons/{{ icon }}.svg"
                alt="{{ icon }}"
                height="20"
                width="20"
            />
        </a>

    </li>

    {% endfor %}

</ul>
